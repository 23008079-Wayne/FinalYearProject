# Stripe Test Mode Keys (replace with your actual keys from https://dashboard.stripe.com/apikeys)
STRIPE_SECRET_KEY=sk_test_YOUR_KEY_HERE
STRIPE_PUBLIC_KEY=pk_test_YOUR_KEY_HERE

# PayPal Sandbox Keys (replace with your actual keys from https://developer.paypal.com/dashboard)
PAYPAL_CLIENT_ID=YOUR_PAYPAL_CLIENT_ID_HERE
PAYPAL_SECRET_ID=YOUR_PAYPAL_SECRET_ID_HERE
PAYPAL_MODE=sandbox

# Finnhub API Key (get free key from https://finnhub.io/)
FINNHUB_API_KEY=your_finnhub_api_key_here

# Open Exchange Rates API Key (get free key from https://openexchangerates.org/)
OPEN_EXCHANGE_RATES_API_KEY=your_open_exchange_rates_api_key_here

# Base URL for redirects
BASE_URL=http://localhost:3000
